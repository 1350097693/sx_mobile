<view class="weui-panel__bd">
  <a class="weui-media-box weui-media-box_appmsg">
    <view class="weui-media-box__hd">
      <image class="weui-media-box__thumb" src="{{userInfo.avatarUrl}}"
        alt></image>
    </view>
    <view class="weui-media-box__bd">
      <h4 class="weui-media-box__title">{{userInfo.nickName}}</h4>
      <view class="weui-media-box__desc">{{userInfo.nickName}}</view>
    </view>
  </a>
</view>

<!-- 下方列表 -->
<view class="weui-cells">
  <a class="weui-cell weui-cell_active weui-cell_access weui-cell_example">
    <view class="weui-cell__hd">
      <image src="data:image/png;base64,"
        alt style="width: 20px; height: 20px; margin-right: 16px; display: block;"></image>
    </view>
    <view class="weui-cell__bd weui-cell_primary">
      <view class="item">设置</view>
    </view>
    <text class="weui-cell__ft"></text>
  </a>
</view>